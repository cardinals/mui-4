<script type="text/javascript">
	var currentURL=getCurrentURL(document);
	var news_id=getQueryStringValue(currentURL,"news_id");
	//news_id='1126559';
	
	$(function(){
		getLiveNewsDetail();
	});
//取得快讯详细
function getLiveNewsDetail(){
	
	var url='https://dedicated.wallstreetcn.com/api/v1/livenews/'+news_id;

	$.ajax({
		url : url,
		type : 'get',
		dataType:"JSON",
		async: false,
		data:{},
		success:function(data){
				
			//成功
			if(data.code=="20000"){
				
				var calendar=data.data;
				
				if(calendar!=null){
					
					var html="";
				    //内容
				    html=html+calendar.content;
				    //时间
				    html=html+getLocalTime(calendar.display_time);
				    if(calendar.image_uris.length>0){
				    	$.each(calendar.image_uris,function(n1,value){
				    		//图片
				    		html=html+value;
				    	});
				    }
			
						$(".detail").html(html);
				}
				
			}
		}
	});
}



</script>
<body>
<div class="frame">

<!-- 头导航 -->
<header class="header-m">
<div class="weui-flex">
  <div class="weui-flex__item hl"><a href="javascript:history.back()"><i class="iconfont icon-back"></i></a></div>
  <div class="weui-flex__item ht">资讯中心</div>
  <div class="weui-flex__item hr"></div>
</div>
</header>
<!-- 头导航 //-->

<div class="detail">
		
</div>
    
    






</div>
</body>
